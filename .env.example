# Blackboard 环境变量配置示例

# PostgreSQL 配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=blackboard
DB_USER=postgres
DB_PASSWORD=password
DB_MIN_CONN=5
DB_MAX_CONN=20

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_MAX_CONNECTIONS=50

# S3/MinIO 配置
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=artifacts

# 缓存配置
CACHE_TTL=3600
